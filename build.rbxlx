		elseif Array.isArray(type_) then
			typeString = "array"
		elseif type_ ~= nil and typeof(type_) == "table" and type_["$$typeof"] == REACT_ELEMENT_TYPE then
			typeString = string.format("<%s />", getComponentName((type_ :: any).type) or "Unknown")
